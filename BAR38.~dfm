  object QDetalleTotales: TADOQuery
    Connection = DM.ADOBar
    CursorType = ctStatic
    Filtered = True
    LockType = ltBatchOptimistic
    BeforeOpen = QDetalleTotalesBeforeOpen
    OnCalcFields = QDetalleCalcFields
    DataSource = dsVentas
    Parameters = <
      item
        Name = 'CajeroID'
        Attributes = [paNullable]
        DataType = ftInteger
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = Null
      end
      item
        Name = 'FacturaID'
        Attributes = [paNullable]
        DataType = ftInteger
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = Null
      end
      item
        Name = 'CajaID'
        Attributes = [paNullable]
        DataType = ftInteger
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = Null
      end>
    SQL.Strings = (
      
        'select sum((Cantidad * Precio)-montoitbis) as subTotal,  sum(Des' +
        'cuento) as descuentoTotal'
      '  from Factura_Items'
      'where CajeroID = :CajeroID'
      '  and FacturaID = :FacturaID'
      '  and CajaID = :CajaID'
      'group by CajeroID, FacturaID, CajaID'
      '')
    Left = 112
    Top = 312
    object QDetalleTotalessubTotal: TFloatField
      FieldName = 'subTotal'
    end
    object QDetalleTotalesdescuentoTotal: TBCDField
      FieldName = 'descuentoTotal'
      Precision = 19
    end
  end
