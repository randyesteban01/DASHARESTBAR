  object QFactura: TADOQuery
    Connection = DM.ADOBar
    AfterOpen = QFacturaAfterOpen
    Parameters = <
      item
        Name = 'fec1'
        Attributes = [paNullable]
        DataType = ftWideString
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = Null
      end
      item
        Name = 'fec2'
        Attributes = [paNullable]
        DataType = ftWideString
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = Null
      end>
    SQL.Strings = (
      'select'
      
        'u.usu_nombre Nombre, f.FacturaID, f.Fecha, f.Exento, f.Grabado, ' +
        'f.Itbis,'
      'f.Descuento, f.Propina, f.Total, f.Estatus, f.NCF, f.Cuadrada,'
      'f.rnc, f.Nombre as Cliente, f.CajaID, f.CajeroID, f.Hora,'
      'p.Forma, p.Monto as Pagado, p.Devuelta, p.tipo_Tarjeta,'
      'p.Autorizacion, p.Numero_Tarjeta, p.Banco, f.credito,'
      'f.tiponcf, f.mesaid'
      'from'
      'Factura_RestBar f, usuarios u, restbar_factura_forma_pago p'
      'where'
      'f.cajeroid = u.usu_codigo'
      'and f.FacturaID = p.FacturaID'
      'and f.CajaID = p.CajaID'
      'and f.CajeroID = p.CajeroID'
      'and f.hora between :fec1 and :fec2'
      'order by'
      'f.FacturaID desc')
    Left = 112
    Top = 240
    object QFacturaNombre: TWideStringField
      FieldName = 'Nombre'
      Size = 255
    end
    object QFacturaFacturaID: TIntegerField
      FieldName = 'FacturaID'
