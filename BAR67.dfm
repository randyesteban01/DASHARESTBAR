  object QVentas: TADOQuery
    Connection = DM.ADOBar
    CursorType = ctStatic
    AfterOpen = QVentasAfterOpen
    Parameters = <
      item
        Name = 'fec1'
        Attributes = [paNullable]
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end
      item
        Name = 'fec2'
        Attributes = [paNullable]
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end>
    SQL.Strings = (
      'select distinct'
      
        'u.usu_nombre Nombre, f.FacturaID, f.Fecha, f.Exento, f.Grabado, ' +
        'f.Itbis,'
      'f.Descuento, f.Propina, f.Total, f.Estatus, f.NCF, f.Cuadrada,'
      'f.rnc, f.Nombre as Cliente, f.CajaID, f.CajeroID, f.Hora'
      
        ', (select sum(d.Cantidad*d.costo) from Factura_Items d where f.f' +
        'acturaid = d.facturaid)as Costo'
      'from'
      'Factura_RestBar f, usuarios u , Restbar_factura_forma_pago p'
      'where f.cajeroid = u.usu_codigo'
      'and f.FacturaID = p.FacturaID'
      'and f.CajaID = p.CajaID'
      'and f.CajeroID = p.CajeroID'
      'and f.hora between :fec1 and :fec2')
    Left = 112
    Top = 240
    object QVentasFacturaID: TIntegerField
