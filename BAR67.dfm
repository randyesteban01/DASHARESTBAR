  object QFPago: TADOQuery
    Connection = DM.ADOBar
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select '
      
        '  sum(case forma when '#39'Efectivo'#39' then monto ELSE 0 end) TEfectiv' +
        'o'
      ' ,sum(case forma when '#39'Tarjeta'#39'  then monto ELSE 0 end) TTarjeta'
      
        ',sum(case forma when '#39'Credito'#39'  then f.Total ELSE 0 end) TCredit' +
        'o '
      'from restbar_factura_forma_pago fp '
      
        'inner join Factura_RestBar f on fp.CajaID = f.CajaID and fp.Caje' +
        'roID = f.CajaID and fp.FacturaID = f.FacturaID')
